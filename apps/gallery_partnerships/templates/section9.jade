- section = sections[9]

.gallery-partnerships-section-header
  .grid-2-up
    .grid-item
      h2= section.title
      h3= section.heading
    .grid-item
      p.preamble!= section.preamble

.gallery-partnerships-section-content.education
  .grid-4-up
    each g in section.education
      .grid-item
        a.education-gallery-image.hoverable-image-link( href=g.href )
          img( src=g.src )
        .education-gallery-info
          .education-gallery-name= g.name
          .education-gallery-location= g.location