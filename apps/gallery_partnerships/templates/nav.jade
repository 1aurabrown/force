.gallery-partnerships-section-nav
  nav
    for section, i in sections.slice(0, -1)
      a.gallery-partnerships-nav-link.internal( href="/gallery-partnerships/#{section.slug}", data-section=section.title )
        span= section.title
    a.gallery-partnerships-nav-link.gallery-partnerships-nav-apply-link.avant-garde-button-black.is-small.apply-link-cta(
      href=((sd.GALLERY_PARTNERSHIPS_APPLY === 'inline') ? '/gallery-partnerships/apply' : '/apply?00NC0000005RNfN=link')
      class=((sd.GALLERY_PARTNERSHIPS_APPLY === 'inline') ? 'internal' : undefined)
      data-section='Apply'
    )
      span=sections[sections.length - 1].title
