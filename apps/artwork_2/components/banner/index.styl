.artwork-banner
  display flex
  justify-content space-between
  align-items center
  block-margins(10px)
  padding 15px
  text-align left
  border-bottom 1px solid gray-lighter-color

  &__logo
    margin-right 10px
    padding-right @margin-right
    border-right 1px solid gray-lighter-color

  &__name
