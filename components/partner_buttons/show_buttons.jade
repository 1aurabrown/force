- typeName = show.partner().typeName()
span.partner-buttons-show-buttons( data-id=show.get('id') )
  if show.get('partner') && show.get('partner').default_profile_public
    button.avant-garde-button-white.follow-button( data-id=show.get('partner').id )
      = " " + typeName
  if typeName == 'Gallery'
    button.avant-garde-button-white.partner-buttons-contact Contact #{typeName}
