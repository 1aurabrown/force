.gallery-partnerships-section-nav
  nav
    for section, i in sections.slice(0, -1)
      a.gallery-partnerships-nav-link.internal( href="/gallery-partnerships/#{section.slug}", data-section=section.title )
        span= section.title
    a.gallery-partnerships-nav-link.apply.avant-garde-button-black.is-small( href="/partner-application" )
      span=sections[sections.length - 1].title
