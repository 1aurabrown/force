a.mlsb-suggestion( class=item.publishedClass()  )
  span.mlsb-suggestion-thumbnail
    span.mlsb-suggestion-fallback
      img( src=item.imageUrl(), width='32', height='32', onerror='this.style.display="none";' )
  span.mlsb-suggestion-value
    span.mlsb-suggestion-display
      = item.display()
    if item.get('model')
      em.mlsb-suggestion-kind
        = item.displayModel()
