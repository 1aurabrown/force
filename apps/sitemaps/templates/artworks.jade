doctype xml
urlset(
  xmlns='http://www.sitemaps.org/schemas/sitemap/0.9'
)
  - slugs = _.pluck(models, 'id')
  for slug in slugs
    url
      loc=sd.APP_URL + '/artwork/' + slug
    url
      //-redirects to '/artwork/slug' if there are no auction results
      loc=sd.APP_URL + '/artwork/' + slug + '/auction-results'
    