@import '../../components/stylus_lib'

.fsfs-metadata
  margin 10px 0 20px 0
  padding-right 20px
  text-decoration none
  display block

.fsfs-partner-name
.fsfs-show-name
.fsfs-running-dates
  overflow ellipsis

.fsfs-show-name
.fsfs-running-dates
  line-height 1.4

.fsfs-partner-name
  avant-garde()
  font-size default-avant-garde-font-size

.fsfs-warning
  color red-color
