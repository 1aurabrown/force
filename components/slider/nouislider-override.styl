.noUi-background
  background #e5e5e5
  box-shadow none

.noUi-horizontal
  height 5px

.noUi-origin
  border-radius 0px

.noUi-target
  border none

.noUi-horizontal .noUi-handle
  width 15px
  height 15px
  border-radius 15px
  left -7px
  box-shadow none
  border 1px solid #000

.noUi-connect
  background #000

.noUi-handle:before, .noUi-handle:after
  display none
